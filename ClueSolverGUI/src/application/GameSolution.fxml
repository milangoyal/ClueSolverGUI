<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GameSolutionController">
   <children>
      <Pane prefHeight="571.0" prefWidth="722.0">
         <children>
            <TableView fx:id="table1" layoutX="8.0" layoutY="96.0" prefHeight="381.0" prefWidth="705.0">
              <columns>
                <TableColumn fx:id="locations" prefWidth="122.0" text="Location" />
                  <TableColumn fx:id="cards" prefWidth="580.0" text="Known Cards" />
              </columns>
            </TableView>
            <Label alignment="CENTER" layoutX="257.0" layoutY="55.0" prefHeight="17.0" prefWidth="218.0" text="Game Solution">
               <font>
                  <Font name="Copperplate" size="25.0" />
               </font>
            </Label>
            <Button fx:id="homeButton" layoutX="317.0" layoutY="485.0" mnemonicParsing="false" onAction="#goToHomeScene" text="Return Home" />
         </children>
      </Pane>
   </children>
</AnchorPane>
